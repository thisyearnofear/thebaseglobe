{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/js/$1"
        },
        {
            "src": "/audio/(.*)",
            "headers": {
                "Content-Type": "audio/mpeg"
            },
            "dest": "/audio/$1"
        },
        {
            "src": "/(.*)\\.css",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "/$1.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}